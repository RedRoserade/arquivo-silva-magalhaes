@model IEnumerable<ArquivoSilvaMagalhaes.Models.ArchiveModels.DigitalPhotograph>

@{
    
    Layout = "~/Areas/BackOffice/Views/Shared/_IndexPageLayout.cshtml";
}
    <table class="table table-striped table-hover">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Specimen.AuthorCatalogationCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ScanDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Notes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MimeType)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Specimen.AuthorCatalogationCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScanDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Notes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MimeType)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    
    </table>
