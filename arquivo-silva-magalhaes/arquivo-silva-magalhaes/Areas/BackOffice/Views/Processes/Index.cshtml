@model IEnumerable<ArquivoSilvaMagalhaes.Models.ArchiveModels.Process>
    

@{
    ViewBag.Title = "Index";
}


<h2>Processos</h2>

<p>
    @Html.ActionLink("Criar novo processo", "Create")
</p>
<table class="table">
    
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LanguageCode)
            </th>
        </tr>

       @foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LanguageCode)
        </td>
        
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.Id }) <br />
            @Html.ActionLink("Detalhes", "Details", new { id = item.Id }) <br />
            @Html.ActionLink("Eliminar", "Delete", new { id = item.Id })
        </td>
    </tr>
}

                </table>
@Html.ActionLink("Início", "../Home")