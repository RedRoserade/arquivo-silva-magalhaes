@* View which asks the user if they want to add more content. *@

@using ArquivoSilvaMagalhaes.Resources


@{
    ViewBag.Title = "A terminar a inserção de conteúdo";
}

<div class="page-header">
    <h1>@UiPrompts.AddContentEnd</h1>
</div>

<p>@UiPrompts.AddNewLanguage</p>


@Html.ActionLink(UiStrings.Yes, "AddLanguage", new { Id = ViewBag.Id }, htmlAttributes: new { @class = "btn btn-primary" })
@Html.ActionLink(UiStrings.No, "Index", null, htmlAttributes: new { @class = "btn btn-default" })

