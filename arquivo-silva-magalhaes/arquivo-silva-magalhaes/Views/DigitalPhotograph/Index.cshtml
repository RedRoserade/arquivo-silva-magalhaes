@model IEnumerable<ArquivoSilvaMagalhaes.Models.ArchiveModels.DigitalPhotograph>



<h3 class="page_title">Fotografias</h3>

<table class="table">

@foreach (var item in Model) {
    <tr>
        <td>
            <img src="~/images/@Url.Content(item.FileName)" />
        </td>
        <td>
            @Html.ActionLink("Informações", "Details", new { id=item.Id })
        </td>
    </tr>
}   
</table>
