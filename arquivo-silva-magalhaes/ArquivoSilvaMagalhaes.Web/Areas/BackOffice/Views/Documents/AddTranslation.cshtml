@model DocumentTranslation

@{
    ViewBag.Title = UiStrings.AddTranslation;
    Layout = "~/Areas/BackOffice/Views/Shared/_EditPageLayout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.PartialFor(model => model, "_DocumentTranslateableFields")

        @Html.Partial("_SubmitBackPartial", UiStrings.Done)
    </div>
}

