@model TranslatedViewModel<Archive, ArchiveTranslation>

@{
    ViewBag.Title = FrontOfficeStrings.History;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="embed-responsive embed-responsive-16by9">
    <iframe class="embed-responsive-item"
            src="//www.youtube-nocookie.com/embed/@(ViewBag.VideoId)?rel=0"
            frameborder="0"></iframe>
</div>

<h2>@FrontOfficeStrings.History</h2>
<hr />
<p>@Html.DisplayFor(model => model.Translation.ArchiveHistory)</p><br />
<h2>@FrontOfficeStrings.Mission</h2>
<hr />
<p>@Html.DisplayFor(model => model.Translation.ArchiveMission)</p><br />